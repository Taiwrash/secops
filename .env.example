# Environment variables template
# Copy this to .env and fill in actual values
# DO NOT commit .env to version control

# Flask secret key for session management
SECRET_KEY=your-secret-key-here-min-32-chars-recommended

# Test secret keys (for development/testing only)
TEST_SECRET_KEY=your-test-secret-key-here
TEST_SECRET_KEY2=your-test-secret-key-2-here

# Token time to live in seconds
TOKEN_TIME_TO_LIVE=60

# Vulnerable mode (1=vulnerable, 0=secure)
VULNERABLE=1
